{
  "name": "fashion-web",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "prepare": "node -e \"try { require('fs').accessSync('.git'); require('child_process').execSync('husky', { stdio: 'inherit' }); } catch(e) { console.log('Skipping husky: .git not found (Docker/CI environment)'); }\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "commit": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "release:first": "standard-version --first-release",
    "prepare-release": "./scripts/prepare-release.sh"
  },
  "dependencies": {
    "@google-cloud/tasks": "^6.2.1",
    "@payloadcms/db-postgres": "^3.68.3",
    "@payloadcms/live-preview-react": "^3.68.3",
    "@payloadcms/next": "^3.68.3",
    "@payloadcms/payload-cloud": "^3.68.3",
    "@payloadcms/plugin-form-builder": "^3.68.3",
    "@payloadcms/plugin-nested-docs": "^3.68.3",
    "@payloadcms/plugin-redirects": "^3.68.3",
    "@payloadcms/plugin-search": "^3.68.3",
    "@payloadcms/plugin-seo": "^3.68.3",
    "@payloadcms/richtext-lexical": "^3.68.3",
    "@payloadcms/ui": "^3.68.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "next": "^15.5.8",
    "payload": "^3.68.3",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.0.3",
    "react-dom": "^19.0.3",
    "react-hook-form": "^7.68.0",
    "sharp": "^0.34.5",
    "swr": "^2.3.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@next/eslint-plugin-next": "^15.5.8",
    "@payloadcms/eslint-config": "^3.28.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^25.0.1",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.5.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.0.3",
      "@types/react-dom": "^19.0.3"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
